<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apptracker">
    <!-- Permisos -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icontrn"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icontrn"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".views.WarzoneActivity"></activity>
        <activity android:name=".views.WarzoneDetail"></activity>
        <activity android:name=".views.ApexActivity"></activity>
        <activity android:name=".views.ApexDetail"></activity>
        <activity android:name=".views.CsgoActivity"></activity>
        <activity android:name=".views.CsgoDetail"></activity>

        <activity android:name=".views.LoginActivity"></activity>
        <activity android:name=".views.RegisterActivity"></activity>
        <activity android:name=".views.ConfirmationActivity"></activity>
        <activity android:name=".views.ForgetActivity"></activity>

        <activity android:name=".views.HistorialActivity"></activity>

        <activity
            android:name=".views.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>